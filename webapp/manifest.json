{
  "_version": "1.73.1",
  "sap.app": {
    "id": "com.sapdev.threed.zsapdev3dpproduct",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.19.3",
      "toolsId": "6c012c6c-91f4-4bf7-bf9c-f162f870d75f"
    },
    "dataSources": {
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zui_sapdev_material_o4/srvd/sap/zui_sapdev_material_o4/0001/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZSapdev3DMaterial-maintain": {
          "semanticObject": "ZSapdev3DMaterial",
          "action": "maintain",
          "title": "{{ZSapdev3DMaterial-maintain.flpTitle}}",
          "subTitle": "{{ZSapdev3DMaterial-maintain.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.136.8",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sapdev.threed.zsapdev3dpproduct.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ZC_SAPDEV_MaterialList",
          "target": "ZC_SAPDEV_MaterialList"
        },
        {
          "pattern": "ZC_SAPDEV_Material({key}):?query:",
          "name": "ZC_SAPDEV_MaterialObjectPage",
          "target": "ZC_SAPDEV_MaterialObjectPage"
        }
      ],
      "targets": {
        "ZC_SAPDEV_MaterialList": {
          "type": "Component",
          "id": "ZC_SAPDEV_MaterialList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/ZC_SAPDEV_Material",
              "variantManagement": "Page",
              "navigation": {
                "ZC_SAPDEV_Material": {
                  "detail": {
                    "route": "ZC_SAPDEV_MaterialObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "ZC_SAPDEV_MaterialObjectPage": {
          "type": "Component",
          "id": "ZC_SAPDEV_MaterialObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/ZC_SAPDEV_Material",
              "content": {
	              "header": {
									"actions": {
										"customHeaderActionShowMessage": {
											"press": ".extension.com.sapdev.threed.zsapdev3dpproduct.ext.controller.ProductExt.GenerateTheBarCode",
											"visible": true,
											"enabled": true,
											"text": "{i18n>showBarcode}"
										}
									}
								},
                "body": {
                  "sections": {
                    "BarCode": {
                      "template": "com.sapdev.threed.zsapdev3dpproduct.ext.fragment.BarCode",
                      "position": {
                        "placement": "After",
                        "anchor": "GeneralInfo"
                      },
                      "title": "Barcode"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ObjectPage.ObjectPageController#com.sapdev.threed.zsapdev3dpproduct::ZC_SAPDEV_MaterialObjectPage": {
            "controllerName": "com.sapdev.threed.zsapdev3dpproduct.ext.controller.ProductExt"
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
